{
  "name": "warp-core",
  "version": "0.0.3",
  "description": "Backend service for online multiplayer game engine",
  "author": "Space Game Engine",
  "private": false,
  "license": "MIT",
  "homepage": "https://github.com/Space-Game-Engine/Warp-Core",
  "bugs": {
    "url": "https://github.com/Space-Game-Engine/Warp-Core/issues"
  },
  "type": "commonjs",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:ci": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test:unit": "jest",
    "test:unit-ci": "jest --ci --reporters='github-actions'",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=test INSTALLATION_DIRECTORY=\"test/e2e/test-install-files\" jest --config test/e2e/jest-e2e.json --runInBand --detectOpenHandles --forceExit",
    "test:e2e-ci": "NODE_ENV=test INSTALLATION_DIRECTORY=\"test/e2e/test-install-files\" jest --config test/e2e/jest-e2e.json --runInBand --detectOpenHandles --forceExit --ci --reporters='github-actions'",
    "typeorm": "typeorm-ts-node-commonjs",
    "install-game": "nestjs-command install"
  },
  "dependencies": {
    "@nestjs/apollo": "^13.1.0",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/graphql": "^13.1.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/typeorm": "^11.0.0",
    "better-sqlite3": "^12.2.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cli-progress": "^3.12.0",
    "config": "^4.1.1",
    "express": "^5.1.0",
    "graphql": "^16.11.0",
    "js-yaml": "^4.1.0",
    "luxon": "^3.7.2",
    "nestjs-cls": "^6.0.1",
    "nestjs-command": "^3.1.5",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.2",
    "sqlite3": "^5.1.7",
    "typeorm": "^0.3.26",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.2",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.35.0",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.7",
    "@nestjs/testing": "^11.1.6",
    "@types/cli-progress": "^3.11.6",
    "@types/express": "^5.0.3",
    "@types/jest": "30.0.0",
    "@types/jest-when": "^3.5.5",
    "@types/js-yaml": "^4.0.9",
    "@types/luxon": "^3.7.1",
    "@types/node": "^24.3.3",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.3",
    "@types/yargs": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-sonarjs": "^3.0.5",
    "eslint-plugin-unused-imports": "^4.2.0",
    "globals": "^16.4.0",
    "jest": "30.1.3",
    "jest-when": "^3.7.0",
    "prettier": "^3.6.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-jest": "29.4.1",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.9.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "roots": [
      "<rootDir>"
    ],
    "moduleNameMapper": {
      "@warp-core/test/(.*)": "<rootDir>../test/$1",
      "@warp-core/(.*)": "<rootDir>/$1"
    }
  },
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "keywords": []
}
